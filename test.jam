; --- Program initialisation ---
push $FF      ; Load 255 into memory
loadreg b sp  ; Load 255 into SP

; ---   Creating variables   ---
loada 5       ; First
loadb 5       ; Second

; ---      Main program      ---
; Jump if A == B
cmp a b       ; Set zero flag
jmpeq 14      ; Jump after quit if equal

; Quit if A != B
signal $f0    ; Quit

; We made it here if A == B
signal $f1    ; Dump A
signal $f0    ; Quit
